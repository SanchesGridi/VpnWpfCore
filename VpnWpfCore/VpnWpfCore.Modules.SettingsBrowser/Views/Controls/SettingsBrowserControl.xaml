<UserControl x:Class="VpnWpfCore.Modules.SettingsBrowser.Views.Controls.SettingsBrowserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:prism="http://prismlibrary.com/"
             mc:Ignorable="d"
             d:DesignHeight="465"
             d:DesignWidth="230"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Border Style="{StaticResource SBC_BorderStyle}">
        <StackPanel Style="{StaticResource SBC_StackPanelStyle}">
            <StackPanel.Children>
                <Button Style="{StaticResource SBC_ButtonStyle}"
                        Content="Clear Console"
                        Command="{Binding ClearConsoleCommand}"/>
            </StackPanel.Children>
        </StackPanel>
    </Border>
</UserControl>
